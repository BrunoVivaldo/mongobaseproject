{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-console" : "off",
        "func-names" : "off",
        "no-underscore-dangle":"off",
        "prefer-template": "off",
        "no-path-concat" : "off"
    }
}
